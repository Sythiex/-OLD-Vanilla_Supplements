package com.sythiex.vanilla_supplements.client;

import org.lwjgl.input.Keyboard;

import com.sythiex.vanilla_supplements.VSCommonProxy;

import net.minecraft.client.settings.KeyBinding;

public class VSClientProxy extends VSCommonProxy
{
	KeyBinding wandMode = new KeyBinding("key.wand_mode.desc", Keyboard.KEY_F, "key.vanilla_supplements.category");
	ClientRegistry.registerKeyBinding(wandMode);
}